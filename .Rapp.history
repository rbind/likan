pp <- function(#
  alpha = 0.2,#
  df = 8, #
  tail = 2#
)#
{#
t <- seq (-4, 4, by = 0.01)#
den <- dt(t, df)#
low <- qt(alpha / 2, df, lower.tail = TRUE)#
high <- qt(alpha / 2, df, lower.tail = FALSE)#
highs <- qt(alpha, df, lower.tail = FALSE)#
seq <- seq(low, high, by = 0.01)#
plot(#
  x = NULL, #
  y = NULL, #
  xlim = c(-4, 4),#
  ylim = c(0, max(den)),#
  yaxs = "i",#
  xaxs = "i",#
  bty = "l",#
  axes = F,#
  xlab = "t value",#
  ylab = "Frqeuency",#
  font.lab = 2#
)#
polygon(#
  x = c(#
    low, #
    low,#
    seq,#
    high,#
    high#
  ),#
  y = c(#
    0,#
    dt(low, df),#
    dt(seq, df),#
    dt(high, df),#
    0#
  ),#
  col = "#f4cdba",#
  border= "white"#
)#
axis(1)#
axis(2)#
lines(#
  x = t,#
  y = den,#
  col = "#CF232A"#
)#
if (tail == 2){#
segments(#
  x0 = c(low, high),#
  y0 = c(0, 0),#
  x1 = c(low, high),#
  y1 = c(0.25, 0.25)#
)		#
} else {#
segments(#
  x0 = c(highs),#
  y0 = c(0),#
  x1 = c(highs),#
  y1 = c(0.25, 0.25)#
)#
}#
#
text(#
  x = 0,#
  y = dt(0, df) /3,#
  labels = bquote(#
    "Middle" #
    ~ .(100 - alpha * 100) #
    ~~ "% of" #
    ~~ italic(t) #
    ~~ "distribution"#
  )#
)#
highh <- formatC(high, digits = 3, format="f")#
text(#
  x = high, #
  y = dt(0, df) /2, #
  labels = bquote(italic(t) == .(highh)), #
  adj = -0.05#
)		#
}
